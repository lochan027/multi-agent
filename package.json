{
  "name": "nebulaarb",
  "version": "1.0.0",
  "description": "NebulaArb - Autonomous DeFi arbitrage system on Ethereum using multi-agent AI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "server": "ts-node src/server.ts",
    "demo": "ts-node src/demo-workflow.ts",
    "demo:build": "npm run build && node dist/demo-workflow.js",
    "frontend": "cd frontend && npm run dev",
    "netlify:dev": "netlify dev",
    "netlify:deploy": "netlify deploy --dir=frontend/dist --functions=netlify/functions",
    "netlify:deploy:prod": "netlify deploy --prod --dir=frontend/dist --functions=netlify/functions",
    "netlify:build": "cd frontend && npm run build && cd ..",
    "netlify:status": "netlify status",
    "netlify:open": "netlify open:site",
    "netlify:logs": "netlify logs",
    "netlify:functions": "netlify functions:list",
    "deploy:full": "npm run netlify:build && npm run netlify:deploy:prod",
    "start:all": "concurrently \"npm run server\" \"npm run frontend\"",
    "build": "tsc",
    "build:functions": "cd netlify && npm install && cd ..",
    "start": "node dist/server.js",
    "start:cli": "node dist/index.js",
    "clean": "rmdir /s /q dist"
  },
  "keywords": [
    "defi",
    "multi-agent",
    "adk-ts",
    "arbitrage",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@netlify/functions": "^2.4.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
